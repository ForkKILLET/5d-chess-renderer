{
  "name": "5d-chess-renderer",
  "author": {
    "name": "Shaun Wu",
    "email": "alexbay218@gmail.com"
  },
  "version": "0.1.0",
  "description": "Pixijs-based renderer to create 5d chess boards based off of 5d-chess-js",
  "main": "./dist/5d-chess-renderer.js",
  "browser": {
    "module-alias/register": false
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently \"http-server -c-1 ./\" \"nodemon --watch src --exec npm run build\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build",
    "build": "browserify ./src/index.js -t aliasify -t imgurify -s ChessRenderer -o ./dist/5d-chess-renderer.js"
  },
  "aliasify": {
    "aliases": {
      "@local": "./src"
    }
  },
  "_moduleAliases": {
    "@local": "./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/alexbay218/5d-chess-renderer.git"
  },
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/alexbay218/5d-chess-renderer/issues"
  },
  "keywords": [
    "chess",
    "5d",
    "5d-chess",
    "javascript",
    "commonjs",
    "npm",
    "node",
    "browser"
  ],
  "homepage": "https://gitlab.com/alexbay218/5d-chess-renderer#readme",
  "dependencies": {
    "bezier-js": "^4.0.3",
    "deepcopy": "^2.1.0",
    "element-resize-event": "^3.0.3",
    "module-alias": "^2.2.2",
    "nanoevents": "^5.1.11",
    "pixi-filters": "^3.2.2",
    "pixi-fps": "^2.0.1",
    "pixi-viewport": "^4.18.1",
    "pixi.js-legacy": "^5.3.7",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "browserify": "^17.0.0",
    "concurrently": "^5.3.0",
    "http-server": "^0.12.3",
    "imgurify": "^2.0.1",
    "nodemon": "^2.0.7",
    "pixi.js": "^5.3.7"
  }
}

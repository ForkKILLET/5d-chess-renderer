{
  "name": "5d-chess-renderer",
  "author": {
    "name": "Shaun Wu",
    "email": "alexbay218@gmail.com"
  },
  "contributors": [
    {
      "name": "Shad Amethyst",
      "url": "https://gitlab.com/adri326"
    }
  ],
  "version": "1.1.1",
  "description": "PIXI.js based renderer to create 5d chess boards based off of 5d-chess-js",
  "main": "./dist/5d-chess-renderer.js",
  "browser": {
    "module-alias/register": false
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently \"http-server -c-1 ./\" \"npm run build-dev\"",
    "test": "canvest --ci ./canvest",
    "test-dev": "canvest",
    "prepare": "npm run build",
    "build": "browserify ./src/index.js -t aliasify -t imgurify -s ChessRenderer -o ./dist/5d-chess-renderer.js",
    "build-dev": "watchify ./src/index.js -d -v -t aliasify -t imgurify -s ChessRenderer -o ./dist/5d-chess-renderer.js"
  },
  "aliasify": {
    "aliases": {
      "@local": "./src"
    }
  },
  "_moduleAliases": {
    "@local": "./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/alexbay218/5d-chess-renderer.git"
  },
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/alexbay218/5d-chess-renderer/issues"
  },
  "keywords": [
    "chess",
    "5d",
    "5d-chess",
    "javascript",
    "commonjs",
    "npm",
    "node",
    "browser",
    "pixijs"
  ],
  "homepage": "https://gitlab.com/alexbay218/5d-chess-renderer#readme",
  "dependencies": {
    "bezier-js": "^4.0.3",
    "deepcopy": "^2.1.0",
    "deepmerge": "^4.2.2",
    "element-resize-event": "^3.0.3",
    "fast-deep-equal": "^3.1.3",
    "module-alias": "^2.2.2",
    "nanoevents": "^5.1.11",
    "pixi-cull": "^1.0.1",
    "pixi-filters": "^3.2.2",
    "pixi-fps": "^2.0.1",
    "pixi-viewport": "^4.18.1",
    "pixi.js-legacy": "^5.3.7",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "5d-chess-js": "^1.1.2",
    "@canvest/canvest-cli": "^1.2.4",
    "aliasify": "^2.1.0",
    "beefy": "^2.1.8",
    "browserify": "^17.0.0",
    "concurrently": "^6.0.0",
    "delay": "^5.0.0",
    "http-server": "^0.12.3",
    "imgurify": "^2.0.1",
    "pixi.js": "^5.3.7",
    "watchify": "^4.0.0"
  }
}
